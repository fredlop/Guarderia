@page
@model Matriculas.App.WebApp.Pages.Administrativo.Grados.DeleteModel
@{
    
}
<h1 class="h3 mb-2 text-gray-800">Modulo de Administracion de grados - Eliminar</h1>

  <p class="mb-4">
       Esta seguro de eliminar el registro.
    </p>
<div>
    <form method="post">
        <input type="hidden" asp-for="Model.IdGrado" />
        <div asp-validation-summary="All" class="text-danger">
        </div>
     <input type="text" asp-for="Model.NombreGrado" class="form-control" disabled />
     <br />
    <button class="btn btn-primary" asp-page-handler="Delete">Si</button>
    <button class="btn btn-light" asp-page-handler="NoDelete">No</button>
    </form>
</div>